<ul class="menu-root">
  <% site.pages.find({ type, category }).sort('order').each(function (p) { %>
    <% var fileName = p.path.replace(/^.+?\/([\w-]+)\.html/, '$1') %>
    <!-- <li><h3>seprate title</h3></li> -->
    <li>
      <a href="<%- url_for(p.path) %>"
        class="sidebar-link<%- page.title === p.title ? ' current' : '' %><%- p.is_new ? ' new' : '' %>"
        >
        <%- p.title %>
      </a>
    </li>
  <% }) %>
</ul>
