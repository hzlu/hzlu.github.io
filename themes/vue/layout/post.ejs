<div class="sidebar">
  <div class="sidebar-inner">
    <ul class="main-menu">
      <%- partial('_partial/main_menu') %>
    </ul>
  </div>
</div>
<div class="content with-sidebar blog post">
  <%- toc(page.content, { list_number: false, class: 'my-toc' }) %>
  <h1><%- page.title %></h1>
  <h4><%- page.date.format('MMM D[,] YYYY') %></h4>
  <%- page.content %>
</div>

